    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/acgbuttontext"
        android:textOff="@string/switchoffacg"
        android:textOn="@string/switchonacg"
        android:id="@+id/acgswitch"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="acgonclick" />

</RelativeLayout>
