                android:text="@string/defaultbtntext"
                android:id="@+id/chatbtn1"
                android:layout_marginStart="113dp"
                android:layout_marginTop="161dp"
                android:textSize="22sp"
                android:contentDescription="@string/defaultbtntext"
                android:visibility="invisible"
                android:onClick="C1OnClick" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/defaultbtntext"
                android:id="@+id/chatbtn2"
                android:layout_marginStart="113dp"
                android:layout_marginTop="161dp"
                android:textSize="22sp"
                android:contentDescription="@string/defaultbtntext"
                android:layout_column="24"
                android:visibility="invisible"
                android:onClick="C2OnClick" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/defaultbtntext"
                android:id="@+id/chatbtn3"
                android:layout_marginStart="113dp"
                android:layout_marginTop="161dp"
                android:textSize="22sp"
                android:contentDescription="@string/defaultbtntext"
